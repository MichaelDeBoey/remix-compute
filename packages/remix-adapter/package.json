{
  "name": "@netlify/remix-adapter",
  "version": "1.0.0",
  "description": "Remix Adapter for Netlify Functions",
  "types": "./build/src/index.d.ts",
  "main": "./build/src/index.js",
  "exports": {
    ".": "./build/src/index.js"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "build/src/**/*.d.ts.map",
    "resources/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "npm run clean && npm run build",
    "build": "tsc --build tsconfig.json",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/remix-compute.git"
  },
  "keywords": [
    "remix",
    "netlify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify/remix-compute/issues"
  },
  "homepage": "https://github.com/netlify/remix-compute#readme",
  "dependencies": {
    "@netlify/functions": "^1.4.0",
    "@netlify/remix-runtime": "^1.0.0",
    "@remix-run/dev": "^1.13.0",
    "@remix-run/node": "^1.13.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
